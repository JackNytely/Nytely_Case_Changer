{
	"name": "nytely-case-changer",
	"displayName": "Nytely Case Changer",
	"publisher": "JackNytely",
	"description": "Text will be changed to the Selected Case",
	"version": "1.0.0",
	"icon": "static/icon.png",
	"author": {
		"name": "JackNytely"
	},
	"markdown": "standard",
	"keywords": [
		"JackNytely",
		"Case Changer",
		"Nytely Case Changer"
	],
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Formatters",
		"Other"
	],
	"activationEvents": [
		"onLanguage:javascript",
		"onLanguage:typescript",
		"onLanguage:html",
		"onLanguage:css",
		"onLanguage:less",
		"onLanguage:typescriptreact",
		"onLanguage:scss",
		"onLanguage:python",
		"onLanguage:markdown",
		"onLanguage:json",
		"onLanguage:javascriptreact",
		"onLanguage:sass",
		"onLanguage:go"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.32.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/JackNytely/Nytely-Case-Changer_VS-Extension.git"
	},
	"contributes": {
		"configuration": {
			"title": "Case Change",
			"properties": {
				"case.setting": {
					"type": "object",
					"default": {
						"variableCase": true,
						"constantCase": true
					},
					"description": "Specifies the case which you want to use."
				}
			}
		}
	}
}
